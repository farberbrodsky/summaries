\begin_layout Standard

\series bold
\size small
שיטת המאסטר:
\series default
 
\begin_inset Formula $T\left(n\right)=a\cdot T\left(\frac{n}{b}\right)+f\left(n\right)$
\end_inset

.
 1: 
\begin_inset Formula $f\left(n\right)=O\left(n^{\log_{b}a-\varepsilon}\right)$
\end_inset

 אז 
\begin_inset Formula $T\left(n\right)=\Theta\left(n^{\log_{b}a}\right)$
\end_inset

 2: 
\begin_inset Formula $f\left(n\right)=\Theta\left(n^{\log_{b}a}\right)$
\end_inset

 אז 
\begin_inset Formula $T\left(n\right)=\Theta\left(n^{\log_{b}a}\cdot\log n\right)$
\end_inset

 3: 
\begin_inset Formula $a\cdot f\left(\frac{n}{b}\right)\le c\cdot f\left(n\right)$
\end_inset

 עבור 
\begin_inset Formula $c<1$
\end_inset

 כלשהו אז 
\begin_inset Formula $T\left(n\right)=\Theta\left(f\left(n\right)\right)$
\end_inset


\end_layout

\begin_layout Standard

\series bold
\size footnotesize
מאסטר 2:
\series default
 
\begin_inset Formula $T\left(n\right)=cn+T\left(\alpha_{1}n+\beta_{1}\right)+T\left(\alpha_{2}n+\beta_{2}\right)$
\end_inset

, 
\begin_inset Formula $0<\alpha_{1},\alpha_{2}<1$
\end_inset

, 1: 
\begin_inset Formula $\alpha_{1}+\alpha_{2}<1\implies T\left(n\right)=\Theta\left(n\right)$
\end_inset

 2: 
\begin_inset Formula $\alpha_{1}+\alpha_{2}=1\implies T\left(n\right)=\Theta\left(n\log n\right)$
\end_inset

 3: 
\begin_inset Formula $\alpha_{1}+\alpha_{2}>1\implies T\left(n\right)=\Theta\left(n^{p}\right)$
\end_inset

 כאשר 
\begin_inset Formula $\alpha_{1}^{p}+\alpha_{2}^{p}=1$
\end_inset

 וחסום על ידי 
\begin_inset Formula $O\left(n^{2}\right)$
\end_inset

.
\end_layout

